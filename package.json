{
  "name": "web5-modules-monorepo",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "dev:console": "pnpm --filter @web5-modules/console dev",
    "dev:keystore": "pnpm --filter @web5-modules/keystore dev",
    "dev:did": "pnpm --filter @web5-modules/did dev",
    "dev:pds": "pnpm --filter @web5-modules/pds dev",
    "preview": "turbo run preview --parallel"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@types/node": "^24.10.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "turbo": "^2.3.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^6.0.0"
  }
}
